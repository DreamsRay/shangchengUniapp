<view class="tabbar" style="{{'background-color:'+(tabBar.bgColor)+';'+('border-top-color:'+(tabBar.borderColor)+';')}}"><block wx:for="{{tabBar.list}}" wx:for-item="item" wx:for-index="index"><block><view data-event-opts="{{[['tap',[['clickTab',['$0'],[[['tabBar.list','',index]]]]]]]}}" class="{{['tabbar-item',item.showBig?'big':'']}}" style="{{'color:'+(item.pagePath=='/'+nowPath?tabBar.selectedColor:tabBar.color)+';'+('background-color:'+(tabBar.bgColor)+';')+('border-color:'+(item.showBig?item.pagePath=='/'+nowPath?tabBar.selectedColor:tabBar.borderColor:'')+';')}}" bindtap="__e"><block wx:if="{{tabBar.showType=='img'}}"><block><image class="tabbar-icon" src="{{item.pagePath=='/'+nowPath?item.selectedIconPath:item.iconPath}}"></image></block></block><block wx:if="{{tabBar.showType=='iconfont'}}"><block><view class="{{['tabbar-icon','iconfont',item.fontClass]}}"></view></block></block><text class="tabbar-text">{{item.text}}</text><block wx:if="{{item.num}}"><text class="tabbar-item-num">{{item.num}}</text></block></view></block></block></view>